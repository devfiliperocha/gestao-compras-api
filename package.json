    "strapi-provider-email-sendgrid": "^3.6.8",
    "strapi-utils": "3.6.8"
  },
  "lint-staged": {
    "*": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --bail"
    ]
  },
  "author": {
    "name": "Ph4ntom"
  },
  "strapi": {
    "uuid": "3863a116-948d-47e4-831b-845d3e5bcb84"
  },
  "engines": {
    "node": ">=10.16.0 <=14.x.x",
    "npm": "^6.0.0"
  },
  "license": "MIT",
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      ".tmp",
      ".cache"
    ],
    "testEnvironment": "node",
    "collectCoverage": false,
    "collectCoverageFrom": [
      "api/**/*.js",
      "config/functions/**/*.js",
      "config/policies/**/*.js",
      "extensions/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
