{
  "extends": ["eslint:recommended", "google"],
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true,
    "browser": false,
    "jest": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": false
    },
    "sourceType": "module"
  },
  "globals": {
    "strapi": true
  },
  "plugins": ["jest"],
  "rules": {
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "linebreak-style": ["error", "unix"],
    "no-console": "error",
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "no-var": "error",
    "max-len":"off",
    "eqeqeq": ["error", "always"],
    "object-curly-spacing": "off",
    "require-jsdoc": "off"
  }
}
